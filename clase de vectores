    miVector.push_back(numero);
    miVector.push_back(numero);
    bool desicion = true;
    while(desicion){
        
        cout << "ingrese un numero al sistema: " << endl;
    cin >>numero;
    
    miVector.push_back(numero);
    
        cout << "desea seguir lanzando 1/0 " << endl;
